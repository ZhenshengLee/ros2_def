{
    "nodes": {
        "camera_preprocessing_1": {
            "config_file": "camera_preprocessing_node_model.json",
            "inputs": {
                "image_raw": "/cameras/1/raw",
                "camerainfo_in": "/cameras/1/camerainfo"
            },
            "outputs": {
                "image_rect": "/cameras/1/rect"
            }
        },
        "detector_1": {
            "config_file": "yolo_node_model.json",
            "inputs": {
                "image": {
                    "type": "approximate_time_sync",
                    "topics": [
                        "/cameras/1/rect",
                        "/cameras/1/camerainfo"
                    ]
                }
            },
            "outputs": {
                "detections": "/detections/camera_1_objects"
            }
        },
        "camera_preprocessing_2": {
            "config_file": "camera_preprocessing_node_model.json",
            "inputs": {
                "image_raw": "/cameras/2/raw",
                "camerainfo_in": "/cameras/2/camerainfo"
            },
            "outputs": {
                "image_rect": "/cameras/2/rect"
            }
        },
        "detector_2": {
            "config_file": "yolo_node_model.json",
            "inputs": {
                "image": {
                    "type": "approximate_time_sync",
                    "topics": [
                        "/cameras/2/rect",
                        "/cameras/2/camerainfo"
                    ]
                }
            },
            "outputs": {
                "detections": "/detections/camera_2_objects"
            }
        },
        "tracking": {
            "config_file": "tracking_node_model.json",
            "inputs": {
                "detections_1": "/detections/camera_1_objects",
                "detections_2": "/detections/camera_2_objects"
            },
            "outputs": {
                "tracks": "tracks"
            }
        }
    }
}